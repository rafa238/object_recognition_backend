<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Loggin</servlet-name>
        <servlet-class>com.rafael.servlets.Loggin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Objects</servlet-name>
        <servlet-class>com.rafael.servlets.Objects</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddImage</servlet-name>
        <servlet-class>com.rafael.servlets.AddImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListImages</servlet-name>
        <servlet-class>com.rafael.servlets.ListImages</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateImage</servlet-name>
        <servlet-class>com.rafael.servlets.UpdateImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteImage</servlet-name>
        <servlet-class>com.rafael.servlets.DeleteImage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Loggin</servlet-name>
        <url-pattern>/Loggin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Objects</servlet-name>
        <url-pattern>/Objects</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddImage</servlet-name>
        <url-pattern>/AddImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListImages</servlet-name>
        <url-pattern>/ListImages</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateImage</servlet-name>
        <url-pattern>/UpdateImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteImage</servlet-name>
        <url-pattern>/DeleteImage</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
